{
    "manifest_version": 2,
    "icons": {
        "128": "images/icon128.png",
        "48": "images/icon48.png",
        "16": "images/icon16.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "downloads",
        "downloads.open",
        "notifications"
    ],
    "web_accessible_resources": [
        "js/*",
        "images/*"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "name": "NoDupes",
    "version": "1.0",
    "description": "Avoid downloading files you already have!"
}
